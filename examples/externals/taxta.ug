    eger @taxta.besilghan ("f") bolsa
        buyruq "f!\n" yezilsun
        x = x + 2
    tamamlansun
    eger @taxta.besilghan ("s") bolsa
        buyruq "s!\n" yezilsun
        x = x - 2
    tamamlansun
